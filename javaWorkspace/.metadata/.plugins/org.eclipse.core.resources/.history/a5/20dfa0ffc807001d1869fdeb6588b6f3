package com.edu.capsul;
/*
 * Encapsulation 패턴
 */

public class MyDate {

	private int month;
	private int day;
	
	public int getMonth() { 
		
		 return month;
	}
	public void setMonth(int month) {
		//필드초기화 직전에 valid한 값이 할당되도록 제어문을 달아준다.
		if(month>0&&month<=12)	this.month = month;
	}
	public int getDay() {
		return day;
	} 
	public void setDay(int day) {
		
		this.day = day;
	}
	
	
	
}
